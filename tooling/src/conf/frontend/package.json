{
	"name":            "sprout",
	"version":         "1.0.0",
	"files":           [
		"dist/"
	],
	"private":         true,
	"module":          "dist/index.esm.js",
	"main":            "dist/index.cjs.js",
	"unpkg":           "dist/index.global.js",
	"engines":         {
		"node": ">=14.0.0",
		"pnpm": ">=5"
	},
	"scripts":         {
		"dev":             "nuxt",
		"build":           "build --wd ./ --name index.js",
		"start":           "nuxt start",
		"generate":        "nuxt generate",
		"lint:js":         "eslint --ext \".js,.vue\" --ignore-path .gitignore --write ./sources/**",
		"lint:style":      "stylelint \"sources/**/*.{vue,css}\" --ignore-path .gitignore --fix",
		"lint":            "npm run lint:js && npm run lint:style",
		"test":            "jest",
		"storybook":       "start-storybook -p 6006",
		"storybook:nuxt":  "npx nuxt storybook",
		"storybook:build": "build-storybook"
	},
	"lint-staged":     {
		"*.{js,vue}":  "eslint",
		"*.{css,vue}": "stylelint",
		"*.*":         "prettier"
	},
	"husky":           {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"dependencies":    {
		"@babel/core":                             "^7.12.16",
		"@babel/eslint-parser":                    "^7.12.16",
		"@babel/plugin-proposal-class-properties": "^7.12.13",

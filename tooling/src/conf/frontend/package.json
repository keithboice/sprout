		"typescript":                              "^4.1.5",
		"vue":                                     "^2.6.4",
		"vue-class-component":                     "^7.2.6",
		"vue-cli-plugin-vuetify-preset-reply":     "^1.0.4",
		"vue-property-decorator":                  "^9.1.2",
		"vuetify":                                 "^2.4.4",
		"vuetify-loader":                          "^1.7.2",
		"webpack":                                 "^5.21.2",
		"webpack-chain":                           "^6.5.1",

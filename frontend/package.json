{
  "name": "sprout",
  "version": "6.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm-run-all build:clean build:vue build:post:dev",
    "build:vue": "vue-cli-service build",
    "build:clean": "rm -rf dist",
    "build:post:dev": "mv -f ./dist/index.html ../inbox.php & mv -f ./dist/inbox.main.* ../ & mv -f ./dist/js/* ../js/inbox/ & mv -f ./dist/img/* ../images/inbox/  ",
    "lint": "vue-cli-service lint",
    "build:css": "npm run css",
    "css": "npm-run-all css-compile css-prefix",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o src/assets/css/",
    "css-lint": "stylelint src/assets/scss/ --fix",
    "css-prefix": "postcss --replace src/assets/css/starter.css --use autoprefixer --map",
    "css-purge": "purgecss --css src/assets/css/starter.css --content index.html --output src/assets/css/",
    "serve:css": "serve --listen 3000",
    "start:css": "npm-run-all --parallel watch:css serve:css",
    "test:css": "npm run css-lint && npm run css",
    "watch:css": "nodemon -e html,scss -x \"npm run css\""
  },
  "dependencies": {
    "@casl/ability": "4.1.6",
    "@casl/vue": "1.1.1",
    "@fullcalendar/daygrid": "5.5.0",
    "@fullcalendar/interaction": "5.5.0",
    "@fullcalendar/list": "5.5.0",
    "@fullcalendar/timegrid": "5.5.0",
    "@fullcalendar/vue": "5.5.0",
    "@vue/composition-api": "1.0.0-beta.22",
    "@vueuse/core": "4.0.0",
    "animate.css": "4.1.1",
    "apexcharts": "3.23.0",
    "autoprefixer": "^10.2.4",
    "axios": "0.21.1",
    "axios-mock-adapter": "1.19.0",
    "bootstrap": "4.5.2",
    "bootstrap-vue": "2.21.1",
    "chart.js": "2.9.4",
    "core-js": "3.8.1",
    "echarts": "4.8.0",
    "jsonwebtoken": "8.5.1",
    "leaflet": "1.6.0",
    "npm-run-all": "^4.1.5",
    "portal-vue": "2.1.7",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-rtl": "1.7.3",
    "prismjs": "1.19.0",
    "purgecss": "^4.0.2",
    "stylelint": "^13.11.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
